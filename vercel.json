{
  "routes": [
    {
      "src": "/blog",
      "dest": "https://blog.mesagroupconsulting.com"
    },
    {
      "src": "/blog/(.*)",
      "dest": "https://blog.mesagroupconsulting.com/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/blog/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, stale-while-revalidate=86400"
        }
      ]
    }
  ]
}
